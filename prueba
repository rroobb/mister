CLASE PERSONA HEREDA HUMANO {
	ATRIBUTOS:
		PUBLICO ENTERO numero3;
		PRIVADO DECIMAL numero4;
		PUBLICO LISTA ENTERO 3 jeje;
	METODOS:
		FUNCION NADA print(ENTERO asdalsd){
			ENTERO hola;
			ASIGNAR hola = "hola";
			RETORNAR 3;
		}
};

CLASE GORDA HEREDA PERSONA {
	ATRIBUTOS:
		PUBLICO ENTERO numero5;
		PRIVADO DECIMAL numero6;
	METODOS:
		FUNCION ENTERO print(){
		GORDA aux, numero10;
		ASIGNAR numero10 = aux;
		RETORNAR -5/16;
		}
};

GORDA marialicia, x, y;
PERSONA rob;
HUMANO polo;

FUNCION DECIMAL perimetro(){}

FUNCION DECIMAL AREA( DECIMAL base, DECIMAL altura){
	ASIGNAR rob = polo;
}

FUNCION DECIMAL AREA2( DECIMAL base, DECIMAL altura){
	RETORNAR 0.43;
}

FUNCION ENTERO INICIO (ENTERO parametro1 , DECIMAL parametro2) {
	PERSONA persona;
	ENTERO num;
	LISTA TEXTO 3 milista = ["HOLA", "COMO", "ESTAS"];
	ENTERO num1, num2, num3 = (4*5+num2);
	TEXTO string="hola", string2="como";

	SI(5>(4/5+num2) && -4==+4 || "hola"!="como"){
		persona.print(&rob); 
	}
	SINO{
		MIENTRAS (x.numero3 > y.numero1){
		ASIGNAR	num1 = num3 + 1;
		}
	}

	SI(5>(4/5+num2) && 4==4 || "hola"!="como"){
		persona.print();
		persona.print2();
		perimetro();
	}

	IMPRIMIR("hola","como", 5+4, num2);
	LEER(milista);
	SI(persona.numero3 > 8) {
		ASIGNAR y = 10;
	}
	
	ASIGNAR num = x.print();

}