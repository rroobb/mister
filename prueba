ENTERO aux = 40;
CLASE PERSONA {
	ATRIBUTOS:
		PUBLICO ENTERO numero3;
		PUBLICO ENTERO numero4;
	METODOS:
		FUNCION NADA print(ENTERO asdalsd){
			TEXTO hola;
			ASIGNAR hola = "hola";
			IMPRIMIR(hola);
			IMPRIMIR(asdalsd);
		}
};

PERSONA rob;

FUNCION ENTERO print (TEXTO param1, ENTERO parametro1 , DECIMAL parametro2, TEXTO p){
	ASIGNAR parametro1 = 2000;
	RETORNAR -10;
	
}
FUNCION ENTERO INICIO (ENTERO parametro1 , DECIMAL parametro2) {

	ENTERO num1 = 5;
	ENTERO num2 = 4;
	TEXTO p = "hola";
	DECIMAL num3;
	PERSONA aux;
	rob.print(num1);
	aux.print(num2);
	ASIGNAR num1 = print("hola", &num2 , num3, &p);
	IMPRIMIR(num2);
	ASIGNAR rob.numero3 = print("hola", &num2 , num3, &p);
	IMPRIMIR(rob.numero3);
	SI (num1 < num2){
		SI (num2 > num1) {
			IMPRIMIR("si");
		}
	}
	SINO{
		IMPRIMIR("no");
	}
	MIENTRAS (num1 < 0 ){
		IMPRIMIR(num1);
		ASIGNAR num1 = num1 + 1;
	}
	RETORNAR 0;

}